#include <iostream>
//size of the stack
const int size = 5;

class Stack {
  private:
    int arr[size];
    int top;
  public:
    Stack() {
      top = -1;
    }
     bool isEmpty() {
      return top == -1;
    }
    void push(int val) {
      if (top == size - 1) {
        std::cout << "Stack Overflow\n";
        return;
      }
      top++;
      arr[top] = val;
    }
    int pop() {
      if (isEmpty()){
        std::cout << "Stack Underflow\n";
        return -1; 
      }
      int val = arr[top];
      top--;
      return val;
    }
};

